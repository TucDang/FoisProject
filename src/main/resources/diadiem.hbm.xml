<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
<hibernate-mapping>
   <class name="com.example.model.diadiem" table="diadiem">      
      <id name="iddiadiem" type="int" column="iddiadiem">
         <generator class="native"/>
      </id>
      <property name="tendiadiem" column="tendiadiem" type="java.lang.String"/>
      
   </class>
</hibernate-mapping>